<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entity.Task" table="task">
    	<id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
    	<property name="wid" type="java.lang.String"/>
        <property name="pid" type="java.lang.String"/>
        <property name="tid" type="java.lang.String"/>
        <property name="name" type="java.lang.String"/>
        <property name="description" type="java.lang.String"/>
        <property name="urlpattern" type="java.lang.String"/>
        <property name="urlgenerate" type="java.lang.String"/>
        <property name="isloop" type="java.lang.String"/>
        <property name="proxy_open" type="java.lang.String"/>
    </class>
</hibernate-mapping>
