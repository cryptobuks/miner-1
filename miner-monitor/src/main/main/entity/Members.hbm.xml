<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="io.cutoutsy.entity.Members" table="members">
    	<id name="uid" type="int" column="uid">
            <generator class="assigned"/>
        </id>
    	<property name="username" type="java.lang.String"/>
        <property name="regdate" type="date"/>
        <property name="credits" type="int"/>
        <property name="gold" type="int"/>
        <property name="upload" type="double"/>
        <property name="download" type="double"/>
        <property name="seednum" type="int"/>
        <property name="keepseed" type="double"/>
        <property name="chip" type="int"/>
        <property name="charactor" type="int"/>
    </class>
</hibernate-mapping>
